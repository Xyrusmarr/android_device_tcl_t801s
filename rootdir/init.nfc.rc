on post-fs-data
    mkdir /data/vendor/nfc 0770 nfc nfc
    chmod 0660 /dev/st21nfc
    chown nfc nfc /dev/st21nfc
    #Begin added by weiqiang.zhou for FR 10100874 on 2021.02.23
    chown nfc nfc /sys/class/deviceinfo/device_info/NFC
    #End added by weiqiang.zhou for FR 10100874 on 2021.02.23

service stflashtool /vendor/bin/STFlashTool -c /vendor/etc/st21nfc_conf.txt
	class late_start
	group nfc
	user nfc
	oneshot
